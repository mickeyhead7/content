{
  "presets": [
    "env",
    "react",
    "stage-2"
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "@packages": "./src/packages/src",
        "@app": "./src"
      }
    }],
    "transform-async-generator-functions",
    "transform-async-to-generator"
  ],
  "ignore": [
    "__tests__/*"
  ]
}
